<%- model_class = User -%>
<div class="page-header">
  <h1><%=t '.title', :default => 'Thanks' %></h1>
</div>

<h5>Adding your details to the CI Server : </h5>
<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @user.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:giturl) %>:</strong></dt>
  <dd><%= @user.giturl %></dd>
</dl>


